/**
 * Register // TODO - converted unique to local
 */
public class Register<T> {
    private local T val;

    public Register(local T val) {
        this.val = val;
    }

    public local T value() {
        return val;
    }

    public void assign(local T val) {
        this.val = val;
    }
}

restriction RRegister for Register {
    allows value;
    allows assign;
}