/**
 * Register // TODO - converted unique to local
 */
public class Register<T> {
    private T val;

    public Register(T val) {
        this.val = val;
    }

    public T value() {
        return val;
    }

    public void assign(T val) {
        this.val = val;
    }
}

restriction RRegister for Register {
    allows value;
    allows assign;
}