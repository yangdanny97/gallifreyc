public class SharedLinkedList<T> {
    public T data;
    public shared[RList1] SharedLinkedList<T> next;

    public SharedLinkedList(T data) {
        this.data = data;
    }

    public T getData() {
        return this.data;
    }

    public T setData(T data) {
        return this.data = data;
    }

    public shared[RList1] SharedLinkedList<T> getNext() {
        return this.next;
    }

    public void setNext(shared[RList1] SharedLinkedList<T> next) {
        this.next = next;
    }
}

